{
  "name": "Neighbor Report",
  "url": "neighbor.report",
  "version": "0.1.4",
  "addedDatetime": 1703570400000,
  "steps": [
    {
      "stepType": "scan",
      "scanType": "templatedUrl",
      "actions": [
        {
          "actionType": "navigate",
          "id": "bbaf8a18-fef8-42a6-9682-747b8ff485b2",
          "url": "https://neighbor.report/${firstName}-${lastName}/${state|stateFull|hyphenated}/${city|hyphenated}"
        },
        {
          "actionType": "extract",
          "id": "0dac4a6d-1291-47c3-97b8-56200f751ac8",
          "selector": ".lstd",
          "profile": {
            "name": {
              "selector": "(.//a[@class='persons'])[1]"
            },
            "age": {
              "selector": ".age"
            },
            "addressFull": {
              "selector": ".i_room"
            },
            "addressCityState": {
              "selector": ".i_home"
            },
            "phone": {
              "selector": ".i_phone"
            },
            "relativesList": {
              "selector": ".i_people",
              "afterText": "Associated persons:",
              "separator": ","
            },
            "profileUrl": {
              "selector": "(.//a[@class='persons'])[1]"
            }
          }
        }
      ]
    },
    {
      "stepType": "optOut",
      "optOutType": "formOptOut",
      "actions": [
        {
          "actionType": "navigate",
          "id": "b1f7f4ab-51b0-4885-ba73-97be0822d0ba",
          "url": "https://neighbor.report/remove"
        },
        {
          "actionType": "fillForm",
          "id": "743afa6c-7dea-4115-934b-bea369307acd",
          "selector": ".form-horizontal",
          "elements": [
            {
              "type": "fullName",
              "selector": ".//input[@name='inputName']"
            },
            {
              "type": "email",
              "selector": ".//input[@name='inputEmail']"
            },
            {
              "type": "profileUrl",
              "selector": ".//input[@name='inputURL']"
            }
          ]
        },
        {
          "actionType": "getCaptchaInfo",
          "id": "24ce0da0-7cc3-47e7-bf8e-6f5fe98b7a91",
          "selector": ".recaptcha-div"
        },
        {
          "actionType": "solveCaptcha",
          "id": "b720de9a-f519-466f-980d-d9c52d8870a2",
          "selector": ".recaptcha-div"
        },
        {
          "actionType": "click",
          "id": "46690938-f112-4091-bd07-b5641e38151f",
          "elements": [
            {
              "type": "button",
              "selector": ".submit-comment"
            }
          ]
        },
        {
          "actionType": "click",
          "id": "07cfed17-9d75-471a-b6a0-0522add35ffa",
          "elements": [
            {
              "type": "button",
              "selector": ".btn-danger"
            },
            {
              "type": "button",
              "selector": ".btn-success"
            }
          ]
        },
        {
          "actionType": "expectation",
          "id": "ebd61347-60e1-4c19-bc41-dd1ce36d3138",
          "expectations": [
            {
              "type": "text",
              "selector": "body",
              "expect": "we've removed your data from the Neighbor Report website"
            }
          ]
        }
      ]
    }
  ],
  "schedulingConfig": {
    "retryError": 48,
    "confirmOptOutScan": 72,
    "maintenanceScan": 240
  }
