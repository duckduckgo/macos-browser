{
  "name": "Neighbor Report",
  "url": "neighbor.report",
  "version": "0.1.5",
  "addedDatetime": 1703570400000,
  "steps": [
    {
      "stepType": "scan",
      "scanType": "templatedUrl",
      "actions": [
        {
          "actionType": "navigate",
          "id": "22f67a44-d538-48a0-b6f4-c783123c6dfa",
          "url": "https://neighbor.report/${firstName}-${lastName}/${state|stateFull|hyphenated}/${city|hyphenated}"
        },
        {
          "actionType": "extract",
          "id": "14ff868b-b6cc-485e-a5cf-93357a63c2ac",
          "selector": ".lstd",
          "profile": {
            "name": {
              "selector": "(.//a[@class='persons'])[1]"
            },
            "age": {
              "selector": ".age"
            },
            "addressFull": {
              "selector": ".i_room"
            },
            "addressCityState": {
              "selector": ".i_home"
            },
            "phone": {
              "selector": ".i_phone"
            },
            "relativesList": {
              "selector": ".i_people",
              "afterText": "Associated persons:",
              "separator": ","
            },
            "profileUrl": {
              "selector": "(.//a[@class='persons'])[1]",
              "identifierType": "path",
              "identifier": "https://neighbor.report/person/${id}"
            }
          }
        }
      ]
    },
    {
      "stepType": "optOut",
      "optOutType": "formOptOut",
      "actions": [
        {
          "actionType": "navigate",
          "id": "477f4e90-686c-400e-8d45-65749ff60b5d",
          "url": "https://neighbor.report/remove"
        },
        {
          "actionType": "fillForm",
          "id": "335b078c-d27f-4045-8a36-2cc0c125d271",
          "selector": ".form-horizontal",
          "elements": [
            {
              "type": "fullName",
              "selector": ".//input[@name='inputName']"
            },
            {
              "type": "email",
              "selector": ".//input[@name='inputEmail']"
            },
            {
              "type": "profileUrl",
              "selector": ".//input[@name='inputURL']"
            }
          ]
        },
        {
          "actionType": "getCaptchaInfo",
          "id": "0ed843a1-2788-454c-a666-17906d2991cb",
          "selector": ".recaptcha-div"
        },
        {
          "actionType": "solveCaptcha",
          "id": "e6f3b3c5-e61c-4e75-878d-cecbaae26013",
          "selector": ".recaptcha-div"
        },
        {
          "actionType": "click",
          "id": "8544dcb5-5bd7-4045-91a8-23bbfad7bfc8",
          "elements": [
            {
              "type": "button",
              "selector": ".submit-comment"
            }
          ]
        },
        {
          "actionType": "click",
          "id": "6748ca30-0044-4e9a-8a35-5d686359688c",
          "elements": [
            {
              "type": "button",
              "selector": ".btn-danger"
            },
            {
              "type": "button",
              "selector": ".btn-success"
            }
          ]
        },
        {
          "actionType": "expectation",
          "id": "a56fcc4c-fdd4-4d92-96b3-a8b89d10ca80",
          "expectations": [
            {
              "type": "text",
              "selector": "body",
              "expect": "we've removed your data from the Neighbor Report website"
            }
          ]
        }
      ]
    }
  ],
  "schedulingConfig": {
    "retryError": 48,
    "confirmOptOutScan": 72,
    "maintenanceScan": 240
  }
}
