            },
            "relativesList": {
              "selector": "(.//div[@class='ls_success-content-extra-info'])[2]",
              "afterText": "Relatives:",
              "separator": ","
            },
            "profileUrl": {
              "selector": ".ls_success-extra-margin"
            }
          }
        }
      ]
    },
