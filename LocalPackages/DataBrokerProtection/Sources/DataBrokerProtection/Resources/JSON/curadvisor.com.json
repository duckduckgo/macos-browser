            },
            "addressCityState": {
              "selector": ".//div[@class='card-body']/dl[dt[text()='Has lived in:']]/dd/ul[@class='list-inline m-0']/li",
              "findElements": true
            },
            "relativesList": {
              "selector": ".//div[@class='card-body']/dl[dt[text()='Related to:']]/dd/ul[@class='list-inline m-0']/li",
              "beforeText": ",",
              "findElements": true
            },
            "profileUrl": {
              "selector": "a"
            }
          }
        }
      ]
    },
    {
      "stepType": "optOut",
      "optOutType": "parentSiteOptOut",
