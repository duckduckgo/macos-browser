{
  "name": "@duckduckgo/youtube",
  "version": "1.0.0",
  "private": true,
  "devDependencies": {
    "@playwright/test": "^1.27.1",
    "esbuild": "^0.15.9",
    "typescript": "^4.8.4"
  },
  "scripts": {
    "test": "npm run build && npm run tsc",
    "test.e2e": "npm run build && playwright test --project e2e",
    "test.integration": "npm run build && playwright test --project integration",
    "_build": "esbuild --loader:.css=text --loader:.svg=text --bundle youtube-inject.js",
    "build": "npm run _build -- --outfile='dist/youtube-inject.bundle.js'",
    "build:watch": "npm run _build -- --outfile='dist/youtube-inject.bundle.js' --watch",
    "tsc": "tsc",
    "tsc:watch": "npm run tsc -- --watch"
  }
}
